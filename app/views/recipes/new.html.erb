<%#= render 'form', recipe: @recipe %>
<%= form_for(@recipe) do |f| %>
    Title: <%= f.text_field :title %>
    <%= f.fields_for :ingredients do |ing| %>
        <p>Ingredient: <%= ing.text_field :name %>
        Quantity: <%= ing.text_field :quantity %></p>
    <% end %>
    <%= f.submit %>
<% end %>